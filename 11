void Swap(int* x, int* y)//指针函数
{
	int z = 0;
	z = *x;//解引用
	*x = *y;
	*y = z;

}
int main()
{
	int a = 10;
	int b = 20;
	Swap(&a, &b);
	printf("a=%d b=%d", a, b);
	return 0;
}
